{
  "Logging": {
    // Provider
    "LogLevel": { // all providers
      "Default": "Information", // log everything at the info level or higher
      "Microsoft.AspNetCore": "Warning" // All Microsoft.AspNetCore.* packages, warning and higher
    },
    "EventLog": { // windows system or whatever OS we're running on
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "ApplicationInsights": { // log system in Azure
      "LogLevel": {
        "Defaul": "Error"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace"
      }
    }
  },
  "AllowedHosts": "*"
}


// log systems Providers: Console, Debug, EventSource, EventLog, ApplicationInsights, AzureAppServicesFile, AzureAppServicesBlob
// log system Levels: Trace = 0, Debug = 1, Information = 2, Warning = 3, Error = 4, Critical = 5, None = 6